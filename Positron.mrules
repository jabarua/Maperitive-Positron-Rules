	target : aeroway terminal
		define
			min-zoom : 10
			fill-color : #d8d8d8
			line-color : #d8d8d8

		draw : fill
	target : aeroway runway
		define
			min-zoom : 10
			line-color : #d8d8d8
			line-width : 11:2;15:20
		draw : line

	target : aeroway taxiway
		define
			min-zoom : 10
			line-color : #d8d8d8
			line-width : 11:1;15:6
		draw : line

	target : glacier
		define
			min-zoom : 10
			text-color : #e8e8e8
		draw : text

	target : academic
		define
			fill-color : #e8e8e8
			line-style : none
		draw : fill
		define
			line-color : #e8e8e8
			line-width : 1
		draw : line
//		draw : text

	target : boundary*
		define
			line-color : #e8e8e8
			line-opacity : 0.5
		if : boundary country
			define
				line-width : 6:1;9:3;10:6
		elseif : boundary province
			define
				min-zoom : 4
				line-width : 4:1;11:3
				line-style : none
		elseif : boundary municipal
			define
				min-zoom : 12
				line-width : 12:1;14:2
				line-style : none
		draw : line

	target : national park
		define
			fill-color : #fafaf8
			fill-opacity : 0.2
			line-style : none
		draw : fill
		define
			line-color : #e8e8e8
			line-opacity : 0.5
			line-width : 3
			line-style : dash
			font-size : 3:3;12:12
			text-color : green
			font-weight : normal
		draw : line
		draw : text

	target : peak
		define
			shape : triangle
			shape-size : 12
			fill-color : #D08F55
			line-style : none
			min-zoom : 10
		draw : shape
		define
			text : @if(name, name @if(ele, "\n(" ele ")"), ele)
			text-color : #ae4242
			min-zoom : 13
			font-size : 10
			text-max-width : 10
			text-offset-vertical : 100%
			text-align-vertical : near
		draw : text

	target : platform
		define
			min-zoom : 10
			line-color : #e8e8e8
			line-width : 3
		draw : line
	target : railway rail
		define
			min-zoom : 10
			line-color : white
			line-width : 10:1;18:1
		draw : line
		define
			min-zoom : 10
			max-zoom : 14
			line-style : dashlong
			line-color : #bbb
			line-width : 10:1;14:1
			border-style : none
			border-color : #bbb
			border-width : 10:0;14:0.25
		draw : line
		define
			min-zoom : 14
			max-zoom : 15
			line-style : dashlong
			line-color : #aaa
			line-width : 1.25
			border-style : solid
			border-color : #aaa
			border-width : 0.5
		draw : line
		define
			min-zoom : 15
			max-zoom : 16
			line-style : dashlong
			line-color : #999
			line-width : 1.5
			border-style : solid
			border-color : #999
			border-width : 0.3
		draw : line
		define
			min-zoom : 16
			max-zoom : 20
			line-style : dashlong
			line-color : #888
			line-width : 16:2;20:4
			border-style : solid
			border-color : #888
			border-width : 0.2
		draw : line




	target : highway private
		define
			min-zoom : 14
			line-width : 4
			line-style : none
			line-color : #cc1111
		draw : line

	target : highway*
		define
			font-size : 10:0.5;15:7;18:10
			font-weight : bold
			text-color : #444

		for : oneway=yes OR oneway=true
			define
				min-zoom : 20
				shape : custom
				shape-def : 60,0,20,-40,20,-15,-60,-15,-60,15,20,15,20,40;Z
				shape-size : 12
				shape-aspect : 0.75
				shape-spacing : 6
				fill-color : #c4c3c1
				line-style : none
			for : oneway=-1
				define
					angle : 180
			draw : shape

		define
			line-style : solid
			line-join : round
			line-start-cap : round
			line-end-cap : round

		if : *motorway*
			if : *link
				define
					line-width : 10:1;13:2;15:6;18:12
			else
				define
					line-width : 10:2;13:3;15:6;18:12
			define
				min-zoom : 10
				line-color : #FFFFFF
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			draw : line
			define
				min-zoom : 10
				max-zoom : 20
				font-family : Tahoma
				text-align-horizontal : center
				text-align-vertical : center
				text : none
				font-size : 12
				text-color : #333
				text-halo-width : 1
				shape : custom
				shape-def : p:#809bc0;pw:5;f:#809bc0;m:-10,-10;l:10,-10;a:10,10,10,10;l:-10,10;a:10,10,-10,-10;z;p:#fcfcfb;pw:2;m:-10,-10;l:10,-10;a:10,10,10,10;l:-10,10;a:10,10,-10,-10;z
				shield-padding-left : 5
				shield-padding-right : 5
				shield-padding-top : 7
				shield-padding-bottom : 7
				shield-resize-mode : free
			draw : shield
		elseif : *trunk*
			define
				min-zoom : 12
				line-color : #FFFFFF
				line-width : 10:1;13:2;15:6;18:12
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			draw : line
			define
				min-zoom : 7
				max-zoom : 12
				border-style : none
			draw : line
		elseif : *primary*
			define
				min-zoom : 10
				line-color : #FFFFFF
				line-width : 10:1;13:3;15:6;18:12
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			draw : line
			define
				min-zoom : 14
				max-zoom : 20
			draw : text
		elseif : *secondary
			define
				min-zoom : 12
				line-color : #FFFFFF
				line-width : 10:1;13:2;15:6;18:12
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			draw : line
			define
				min-zoom : 8.5
				max-zoom : 12
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			draw : line
			define
				min-zoom : 14
				max-zoom : 20
			draw : text
		elseif : *tertiary
			define
				min-zoom : 13
				line-color : #FFFFFF
				line-width : 10:1;13:2;15:6;18:12
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			draw : line
			define
				min-zoom : 13
				max-zoom : 13
				line-color : #FFFFFF
				border-style : solid
			draw : line
			define
				min-zoom : 14
				max-zoom : 20
			draw : text
		elseif : *service
			define
				min-zoom : 13
				line-color : #FFFFFF
				line-width : 7:0.6;9:1;10:1.2;12:2.5;13:3;16:5;18:6
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			draw : line
		elseif : *pedestrian
			define
				min-zoom : 13
				line-color : #FFFFFF
				line-width : 7:0.6;9:1;10:1.2;12:2.5;13:3;16:5;18:6
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			draw : line

		elseif : $regex("(.*residential)|(.*unclassified)")
			define
				min-zoom : 13
				line-color : #FFFFFF
				line-width : 10:1;13:2;15:5;18:10
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			draw : line
			define
				min-zoom : 10
				max-zoom : 13
				border-color: #777
				border-style : solid
				border-width : 0%
			draw : line
			define
				min-zoom : 14
				max-zoom : 20
			draw : text
		elseif : *living_street
			define
				min-zoom : 14
				line-color : #FFFFFF
				line-width : 10:1;13:2;15:5;18:10
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			draw : line
			define
				min-zoom : 10
				max-zoom : 13
				border-style : none
				line-color : #FFFFFF
			draw : line
			define
				min-zoom : 14
				max-zoom : 20
			draw : text
		elseif : *construction
			define
				min-zoom : 13
				line-color : #FFFFFF
				line-width : 10:1;13:2;15:6;18:12
				line-style : dot
				border-style : solid
				border-color : #dddddd
				border-width : 0%
			for : tunnel=yes
				define
					border-style : dot
			//draw : line
			define
				min-zoom : 10
				max-zoom : 13
				border-style : none
				line-color : #FFFFFF
			draw : line
			define
				min-zoom : 14
				max-zoom : 20
			draw : text
		elseif : *track
			define
				min-zoom : 12.5
				line-color : #fff
				line-width : 1.5
				line-style : none
				border-style : solid
				border-color : #dddddd
				border-width : 0%
				border-opacity : 0.3
			for : tunnel=yes
				define
					border-style : dot
			//draw : line
		elseif : *footway
			define
				min-zoom : 13
				line-color : #d9dad9
				line-width : 13:1;15:2;18:3
				line-style : dot
				border-style : solid
				border-color : #fff
				border-width : 0%
				border-opacity : 0.3
			for : tunnel=yes
				define
					border-style : dot
			//draw : line
			define
				min-zoom : 18
				max-zoom : 20			
			draw : text
		elseif : *cycleway
			define
				min-zoom : 13
				line-color : #FFFFFF
				line-width : 1.5
				line-style : dot
				border-style : solid
				border-color : #999
				border-width : 0%
				border-opacity : 0.3
			for : tunnel=yes
				define
					border-style : dot
			draw : line
		elseif : *path
			define
				min-zoom : 13
				line-color : #fff
				line-width : 1.5
				line-style : none
				border-style : solid
				border-color : white
				border-width : 100%
				border-opacity : 0.3
			for : tunnel=yes
				define
					border-style : dot
			draw : line
		else
			stop

	target : highway steps
		define
			min-zoom : 16
			shape : custom
			shape-def : -1,1, -1,-1, 1,-1, 1,1;Z
			shape-spacing : 2
			shape-size : 16:1;20:3
			shape-aspect : 3
			fill-color : #ccc
			line-color : white
			line-style : none
		draw : shape

	target : line barrier
		define
			min-zoom : 13
			line-color : #e8e8e8
			line-width : 1
		draw : line

	target : bridge foot
